	<string name="app_name_main">Noti-Manager</string>
    
    
	<style name="mainChecker">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textAppearance">?android:attr/textAppearanceSmallPopupMenu</item>
	</style>
	<style name="viewBtn">
		<item name="android:scaleType">fitCenter</item>
		<item name="android:background">@drawable/viewbutton</item>
